<apex:page controller="Wizard" >
   <apex:form >
    <apex:pageBlock title="Please Enter Contact Details">
    	<apex:pageBlockSection columns="1">
            <apex:inputField value="{!c.FirstName}" />
            <apex:inputField value="{!c.LastName}" />
            <apex:inputField value="{!c.Email}" />
            <apex:inputField value="{!c.Phone}" />
        </apex:pageBlockSection>
        <apex:pageBlockButtons location="Top" >
                        <apex:commandButton value="Previous Page" action="{!account_page}" />
        </apex:pageBlockButtons>
                <apex:pageBlockButtons location="bottom" >
        		<apex:commandButton value="Next Page" action="{!opportunity_page}" />
        </apex:pageBlockButtons>

</apex:pageBlock>
       </apex:form> 
</apex:page>